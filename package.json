{
  "name": "cat-ui",
  "version": "3.0.0",
  "license": "ISC",
  "keywords": [
    "design system",
    "angular",
    "componentes",
    "alert",
    "button",
    "chip",
    "common",
    "confirm",
    "datatable",
    "dialog",
    "dropdown",
    "componente dinâmico",
    "painel expansivo",
    "formulário dinâmico",
    "loaders",
    "janela lateral",
    "stepper",
    "tab",
    "tooltip",
    "toolbar",
    "csv",
    "xlsx",
    "file"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node .github/scripts/publish.js && npm run format",
    "build-doc": "ng build cat-ui --configuration production --build-optimizer",
    "format": "prettier --write \"dist/@catrx/ui/package.json\"",
    "eol-ajust": "prettier --end-of-line lf --write .",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analize-bundle": "ng build cat-ui --stats-json && webpack-bundle-analyzer dist/cat-ui/stats.json",
    "lint": "ng lint --fix",
    "postversion": "git push && git push --tags",
    "deploy:hotfix": "npm version patch",
    "deploy:feature": "npm version minor",
    "deploy:release": "npm version major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@fortawesome/fontawesome-free": "~6.2.1",
    "@koalarx/utils": "2.0.8",
    "@ng-select/ng-select": "^12.0.4",
    "animate.css": "~4.1.1",
    "b64-to-blob": "1.2.19",
    "bootstrap": "^5.2.3",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "jwt-decode": "3.1.2",
    "jwt-encode": "1.0.1",
    "ng2-search-filter": "~0.5.1",
    "ngx-mask": "^16.2.4",
    "ngx-pagination": "~6.0.3",
    "ngx-papaparse": "^7.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^17.0.2",
    "node-fs-extra": "^0.8.2",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.7.0"
  }
